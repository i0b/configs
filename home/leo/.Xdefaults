! --- [ Aterm ] ---

Aterm*loginShell:           true
Aterm*transparent:          true
Aterm*geometry:             130x50
Aterm*shading:              10
Aterm*background:           #000000
Aterm*foreground:           #ffffff
Aterm*scrollBar:            false
Aterm*scrollBar_right:      true
Aterm*transpscrollbar:      true
Aterm*borderwidth:          0
Aterm*saveLines:            5500
Aterm*font:                 -misc-fixed-medium-r-normal-*-*-140-*-*-c-*-iso8859-1
!Aterm*font:                *-*-dejavu sans mono-medium-r-normal-*-*-*-*-*-*-*-iso8859-1
!Aterm*boldFont:            *-*-dejavu sans mono-bold-r-normal-*-*-*-*-*-*-*-iso8859-1
!Aterm*font:                -*-*-*-*-*-sans-12-*-*-*-*-*-iso8859-1
Aterm*fading:               70
Aterm*cursorColor:          #d8d8d8
Aterm*tinting:              gray


! --- [ XTerm ] ---

XTerm*jumpScroll:           true
! multiScroll: asynchronous scolling -> faster XTerm
XTerm*multiScroll:          true
! colors
XTerm*foreground:           white
XTerm*background:           black
! font
XTerm*faceName:             Bitstream Vera Sans mono 
XTerm*faceSize:             11
XTerm*boldFont:             Bitstream Vera Sans mono 
XTerm*saveLines:            5500
XTerm*boldColors:           true
XTerm*boldMode:             true
!XTerm*colorBD:             red
!XTerm*colorBDMode:         true
XTerm*underLine:            true
XTerm*colorULMode:          true
XTerm*scrollTtyOutput:      false
XTerm*scrollKey:            true
XTerm*highlightColor:       black
XTerm*cursorBlink:          true
XTerm*cursorColor:          #D8D8D8
!XTerm*pointerColor:        Red
XTerm*internalBorder:       3
XTerm*scrollBar:            false
XTerm*rightScrollBar:       true
XTerm*loginShell:           true
XTerm*geometry:             110x38
!XTerm*charClass:           33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48
XTerm*colorUL:              blue
XTerm*dynamicColors:        on
XTerm*highlightSelection:   true
XTerm*eightBitInput:        false
XTerm*eightBitOutput:       true
XTerm*modifier:             alt
XTerm*titleInhibit:         true
XTerm*visualBell:           false
XTerm*cutNewline:           false
XTerm*cutToBeginningOfLine: false


