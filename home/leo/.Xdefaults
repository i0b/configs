! --- [ misc ] ---

XClock*update:        1
XClock*background:    White
XClock*foreground:    Black
XClock*hands:         Gray80
XClock*hourColor:     Blue
XClock*minuteColor:   Red
XClock*majorColor:    Black

Xft.dpi:              96
Xft.antialias:        true
Xft.hinting:          true
Xft.hintstyle:        hintfull
Xft.rgba:             rgb

! --- [ URxvt ] ---

URxvt.geometry:       115x40
!URxvt.background:    #000000
URxvt.background:     rgba:0000/0000/0000/f1aa
URxvt.foreground:     #ffffff
URxvt.depth:          32
URxvt.internalBorder: 5
URxvt.transparent:    true
URxvt.inheritPixmap:  true
!URxvt.inheritPixmap: false
URxvt.reverseVideo:   false
URxvt.urgentOnBell:   true
URxvt.tintColor:      #dddddd
URxvt.shading:        5
URxvt.fading:         50
!URxvt.font:          -xos4-terminus-medium-r-*-*-14-*-*-*-*-*-iso8859-15
!URxvt.font:          xft:Bitstream Vera Sans mono:size=10
!URxvt.font:          xft:FreeSans:antialias=true:pixelsize=10.7
!URxvt.font:          xft:DejaVu Sans Mono-11:dpi=81
!URxvt.boldFont:      xft:DejaVu Sans Mono-11:bold:dpi=81
!URxvt.itanicFont:    xft:DejaVu Sans Mono-11:itanic:dpi=81
!URxvt.boldItanicFont:xft:DejaVu Sans Mono-11:boldintanic:dpi=81
URxvt.font:           xft:DejaVu Sans Mono-11
URxvt.boldFont:       xft:DejaVu Sans Mono-11:bold
URxvt.itanicFont:     xft:DejaVu Sans Mono-11:itanic
URxvt.boldItanicFont: xft:DejaVu Sans Mono-11:boldintanic

URxvt.secondaryScroll:    true
URxvt.scrollWithBuffer:   true
!URxvt.scrollBar:         true
URxvt.scrollBar:          false
URxvt.scrollBar_floating: true
URxvt.scrollBar_right:    true
URxvt.scrollColor:        #242424
URxvt.scrollTtyKeypress:  false
URxvt.scrollTtyOutput:    false
URxvt.saveLines:          5000

URxvt.cursorBlink:        true
URxvt.cursorColor:        #d8d8d8
URxvt.throughColor:       #8080f0
!URxvt.highlightColor:    #808080
URxvt.boldColors:         true
URxvt.boldMode:           true
URxvt.colorBD:            #707070
URxvt.colorBDMode:        true
!URxvt.colorIT:           lightgreen
URxvt.underLine:          true
!URxvt.colorUL:            #1FA638
URxvt.colorUL:            #D2BC41
URxvt.colorULMode:        true

URxvt.visualBell:         false
!URxvt.perl-lib:          /usr/lib/urxvt/perl/
!URxvt.perl-ext-common:   digital-clock
URxvt.perl-ext-common:    default,matcher,selection-autotransform
!URxvt.perl-ext:          default,matcher
URxvt.urlLauncher:        /usr/bin/links
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/vi +$2 \\Q$1\\E\\x0d/

!colors!
